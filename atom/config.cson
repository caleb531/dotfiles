    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-"
".css.source.styled":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-"
".html.liquid.text":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-"
".html.mustache.sca.ssp.text":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-"
".html.mustache.sca.text.tpl":
