"*":
  "advanced-open-file":
    createDirectories: true
  "atom-package-deps":
    ignored: [
      "linter-ui-default"
    ]
  "autocomplete-plus":
    fileBlacklist: [
      ".env"
    ]
    includeCompletionsFromAllBuffers: false
    maxVisibleSuggestions: 5
  "command-palette":
    preserveLastSearch: true
  core:
    allowPendingPaneItems: false
    closeEmptyWindows: false
    disabledPackages: [
      "archive-view"
      "autocomplete"
      "autosave"
      "background-tips"
      "bookmarks"
      "dev-live-reload"
      "git-diff"
      "language-perl"
      "open-on-github"
      "release-notes"
      "script"
      "timecop"
      "update-package-dependencies"
      "welcome"
      "whitespace"
      "teletype"
    ]
    followSymlinks: false
    ignoredNames: [
      "._*"
      ".DS_Store"
      ".fseventsd"
      ".git"
      ".hg"
      ".sass-cache"
      ".Spotlight-V100"
      ".svn"
      ".virtualenv"
      ".Trashes"
      "DeployDistribution"
      "LocalDistribution"
      "node_modules"
      "Thumbs.db"
    ]
    packagesWithKeymapsDisabled: [
      "atom-alignment"
      "atom-transpose"
      "autoflow"
      "bracket-matcher"
      "emmet"
      "encoding-selector"
      "go-to-line"
      "grammar-selector"
      "link"
      "linter"
      "merge-conflicts"
      "resize-indent"
    ]
    projectHome: "~/Repositories"
    telemetryConsent: "limited"
    themes: [
      "one-dark-ui"
      "monokai"
    ]
    titleBar: "custom"
  docblockr:
    align_tags: "shallow"
    extend_double_slash: false
  editor:
    autoIndentOnPaste: false
    fontFamily: "Monaco"
    fontSize: 16
    showCursorOnSelection: false
    softTabs: false
    softWrap: true
    tabLength: 4
  emmet:
    extensionsPath: "~/dotfiles/emmet"
    useEmmetComments: false
  "exception-reporting":
    userId: "f12f97d7-4abc-ae8e-e993-bf2f40749e71"
  "find-and-replace":
    showSearchWrapIcon: false
  "fuzzy-finder":
    ignoredNames: [
      "*.alfredworkflow"
      "*.docx"
      "*.jpg"
      "*.map"
      "*.out"
      "*.pages"
      "*.pdf"
      "*.png"
    ]
    preserveLastSearch: true
  linter:
    lintOnChange: false
    lintPreviewTabs: false
  "linter-jscs":
    onlyConfig: true
    preset: "jquery"
  "linter-jshint":
    disableWhenNoJshintrcFileInPath: true
  "linter-ui-default":
    hidePanelWhenEmpty: true
    showPanel: true
    showTooltip: false
  "markdown-preview":
    useGitHubStyle: true
  "one-dark-ui":
    tabSizing: "Even"
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  pigments:
    ignoredScopes: [
      "\\.comment"
      "\\.string"
      "\\.source\\.gfm"
    ]
    markerType: "native-dot"
  tabs:
    enableMruTabSwitching: false
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".basic.html.text":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-"
".html.mustache.sca.text.tpl":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-"
".html.php.text":
  editor:
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-"
    tabLength: 4
".python.regexp.source":
  editor:
    preferredLineLength: 79
".python.source":
  editor:
    preferredLineLength: 79
