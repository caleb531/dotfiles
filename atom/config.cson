"*":
  core:
    disabledPackages: [
      "archive-view"
      "background-tips"
      "deprecation-cop"
      "dev-live-reload"
      "welcome"
      "update-package-dependencies"
      "timecop"
      "exception-reporting"
      "image-view"
      "metrics"
      "open-on-github"
      "release-notes"
      "styleguide"
      "autocomplete"
      "tree-view"
      "bookmarks"
      "autosave"
    ]
    followSymlinks: true
    projectHome: "~"
    themes: [
      "atom-dark-ui"
      "atom-dark-syntax"
    ]
  editor:
    fontFamily: "Monaco"
    tabLength: 4
    softTabs: false
    softWrap: true
    softWrapAtPreferredLineLength: true
    invisibles: {}
    autoIndentOnPaste: false
  welcome:
    showOnStartup: false
  linter:
    statusBar: "Show all errors"
    showHighlighting: false
    showErrorInline: false
    lintOnChange: false
    lintOnEditorFocus: false
  autosave:
    enabled: true
  "linter-jshint": {}
  tabs: {}
  whitespace: {}
  script:
    enableExecTime: false
  "linter-pep8": {}
  emmet:
    extensionsPath: "~/.emmet"
  "fuzzy-finder": {}
  "linter-jscs":
    onlyConfig: true
    preset: "jquery"
".applescript.source":
  editor:
    commentStart: "-- "
".ini.source":
  editor:
    commentStart: "# "
".source.viml":
  editor:
    commentStart: "\" "
