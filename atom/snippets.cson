    'body': 'if ( ${1:condition} ) {\n\t$2\n} $3'

  'else if':
    'prefix': 'elseif'
    'body': 'else if ( ${1:condition} ) {\n\t$2\n} $3'

  'else':
    'prefix': 'else'
    'body': 'else {\n\t$1\n}'

  'isset':
    'prefix': 'iss'
