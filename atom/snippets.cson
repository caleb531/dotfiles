  'not empty':
    'prefix': 'nem'
    'body': '! empty( $1 )'

  'for':
    'prefix': 'for'
    'body': 'for ( ${1:\$i} = ${2:0}; ${1:\$i} < ${3:end}; ${1:\$i} += ${4:1} ) {\n\t$5\n}'

  'foreach':
    'prefix': 'foreach'
    'body': 'foreach ( ${1:\$object} as ${2:\$value} ) {\n\t$3\n}'

  'while':
    'prefix': 'while'
    'body': 'while ( ${1:condition} ) {\n\t$2\n}'

  'do..while':
    'prefix': 'dowhile'
