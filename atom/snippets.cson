  'try':
    'prefix': 'try'
    'body': 'try {\n\t$1\n} $2'

  'catch':
    'prefix': 'catch'
    'body': 'catch ( ${1:Exception} \\$${2:e} ) {\n\t$3\n} $4'

  'array (one-line)':
    'prefix': 'ar'
    'body': 'array( $1 )$2'

  'array (one-line; space-padded)':
    'prefix': 'ars'
    'body': ' array( $1 ) '

  'array (empty)':
    'prefix': 'are'
    'body': 'array()'

  'array (empty; space-padded)':
    'prefix': 'ares'
    'body': ' array() '

  'array (multiline)':
    'prefix': 'arn'
    'body': 'array(\n\t$1\n)$2'

  'array (multiline; space-padded)':
    'prefix': 'arns'
    'body': ' array(\n\t$1\n) '

  'array (this)':
    'prefix': 'arthis'
    'body': 'array( $this, \'${1:fname}\' )'

  'key-value pair':
    'prefix': 'kv'
    'body': '\'${1:key}\' => ${2:value}$3'

  'define constant':
    'prefix': 'def'
    'body': 'define( \'$1\', ${2:false} );'

  'define constant conditionally':
    'prefix': 'ifndef'
    'body': 'if ( ! defined( \'$1\' ) ) {\n\tdefine( \'$1\', ${2:false} );\n}'

  'display errors':
    'prefix': 'derr'
    'body': 'ini_set( \'display_errors\', \'1\' );'

  'var_dump':
    'prefix': 'vd'
    'body': 'var_dump( $1 );'

# PHP markup (outside PHP blocks)

'.text.html.php':

  'PHP Block (inline)':
    'prefix': 'php'
    'body': '<?php $1 ?>$2'

  'PHP Block (multiline)':
    'prefix': 'phpblock'
    'body': '<?php\n\t$1\n?>$2'

  'PHP Block (HTML)':
    'prefix': 'phphtml'
    'body': '?>\n\t$1\n<?php$2'

  'echo (inline)':
    'prefix': 'echo'
    'body': '<?php echo $1; ?>$2'

  'if (inline)':
    'prefix': 'if'
    'body': '<?php if ( ${1:condition} ): ?>$2'

  'else (inline)':
    'prefix': 'else'
