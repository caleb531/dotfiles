// Place your settings in this file to overwrite the default settings
{
  "workbench.colorTheme": "Monokai",
  "editor.fontFamily": "Monaco, 'Courier New', monospace",
  "editor.minimap.enabled": false,
  "editor.scrollBeyondLastLine": false,
  "editor.renderWhitespace": "none",
  "security.workspace.trust.enabled": false,
  "workbench.startupEditor": "none",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "support.type.custom-property.less",
          "support.type.custom-property.name.less"
        ],
        "settings": {
          "fontStyle": "",
          "foreground": "#F8F8F2"
        }
      },
      {
        "scope": ["storage.type", "support.class", "variable.parameter"],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "punctuation.accessor.optional.tsx",
          "punctuation.accessor.optional.ts",
          "punctuation.accessor.optional.js",
          "punctuation.accessor.optional.jsx"
        ],
        "settings": {
          "foreground": "#F92672"
        }
      },
      {
        "scope": ["support.type.property-name.json"],
        "settings": {
          "fontStyle": "",
          "foreground": "#A6E22E"
        }
      },
      {
        "scope": ["meta.structure.dictionary.json string.quoted.double.json"],
        "settings": {
          "foreground": "#E6DB74"
        }
      },
      {
        "scope": ["meta.function-call.generic.python"],
        "settings": {
          "foreground": "#A6E22E"
        }
      },
      {
        "scope": [
          "entity.other.attribute-name.table.toml",
          "support.type.property-name.table.toml"
        ],
        "settings": {
          "foreground": "#A6E22E",
          "fontStyle": ""
        }
      },
      {
        "scope": ["entity.name.tag.toml", "support.type.property-name.toml"],
        "settings": {
          "foreground": "#F92672",
          "fontStyle": ""
        }
      },
      {
        "scope": ["support.other.escape.special.regexp"],
        "settings": {
          "foreground": "#AE81FF"
        }
      },
      {
        "scope": [
          "string.quoted.double.sql",
          "string.quoted.other.backtick.sql"
        ],
        "settings": {
          "foreground": "#FD971F"
        }
      },
      {
        "scope": ["punctuation.definition.variable.svelte"],
        "settings": {
          "foreground": "#A6E22E"
        }
      },
      {
        "scope": ["entity.tag.apacheconf"],
        "settings": {
          "foreground": "#F92672"
        }
      },
      {
        "scope": [
          "keyword.core.apacheconf",
          "keyword.mpm.apacheconf",
          "keyword.access.apacheconf",
          "keyword.actions.apacheconf",
          "keyword.alias.apacheconf",
          "keyword.auth.apacheconf",
          "keyword.auth_anon.apacheconf",
          "keyword.auth_dbm.apacheconf",
          "keyword.auth_digest.apacheconf",
          "keyword.auth_ldap.apacheconf",
          "keyword.autoindex.apacheconf",
          "keyword.filter.apacheconf",
          "keyword.cache.apacheconf",
          "keyword.cern_meta.apacheconf",
          "keyword.cgi.apacheconf",
          "keyword.cgid.apacheconf",
          "keyword.charset_lite.apacheconf",
          "keyword.dav.apacheconf",
          "keyword.deflate.apacheconf",
          "keyword.dir.apacheconf",
          "keyword.disk_cache.apacheconf",
          "keyword.dumpio.apacheconf",
          "keyword.env.apacheconf",
          "keyword.expires.apacheconf",
          "keyword.ext_filter.apacheconf",
          "keyword.file_cache.apacheconf",
          "keyword.filter.apacheconf",
          "keyword.headers.apacheconf",
          "keyword.imap.apacheconf",
          "keyword.include.apacheconf",
          "keyword.isapi.apacheconf",
          "keyword.ldap.apacheconf",
          "keyword.log.apacheconf",
          "keyword.mem_cache.apacheconf",
          "keyword.mime.apacheconf",
          "keyword.misc.apacheconf",
          "keyword.negotiation.apacheconf",
          "keyword.nw_ssl.apacheconf",
          "keyword.proxy.apacheconf",
          "keyword.rewrite.apacheconf",
          "keyword.setenvif.apacheconf",
          "keyword.so.apacheconf",
          "keyword.ssl.apacheconf",
          "keyword.substitute.apacheconf",
          "keyword.usertrack.apacheconf",
          "keyword.vhost_alias.apacheconf",
          "keyword.php.apacheconf"
        ],
        "settings": {
          "foreground": "#66D9EF"
        }
      }
    ]
  },
  "workbench.list.openMode": "doubleClick",
  "editor.fontSize": 16,
  "files.associations": {
    "ssh_config": "ssh_config",
    "*.toc": "latex",
    "*.sublime-project": "json",
    "*.har": "json",
    "*.conf": "properties",
    "httpd.conf": "apacheconf",
    "httpd-*.conf": "apacheconf",
    "requirements.txt": "properties",
    "gitignore_global": "ignore",
    ".env*": "properties",
    ".vercelignore": "ignore",
    "spotlight-exclusions.txt": "ignore",
    "*.peggy": "pegjs",
    "yarn.lock": "yaml",
    ".webextrc": "json",
    "config": "properties",
    "*.share": "json",
    ".aider.input.history": "properties",
    "*.ucf": "properties"
  },
  "editor.wordWrap": "bounded",
  "extensions.ignoreRecommendations": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "files.exclude": {
    "**/.virtualenv": true,
    "**/*.map": true,
    "**/DeployDistribution": true,
    "**/DeployDistributionAdvanced": true,
    "**/DeployDistributionAdvancedSS2": true,
    "**/htmlcov": true,
    "**/LocalDistribution": true,
    "**/LocalDistributionAdvanced": true,
    "**/node_modules": true,
    "/.aider*": true
  },
  "editor.occurrencesHighlight": "off",
  "editor.selectionHighlight": false,
  "explorer.autoReveal": "focusNoScroll",
